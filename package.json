{
  "name": "multi-restaurant-delivery-platform",
  "version": "1.0.0",
  "description": "A comprehensive multi-restaurant delivery platform with live tracking, auto-assignment, load balancing, fraud detection, and route optimization",
  "main": "index.js",
  "scripts": {
    "start": "node src/gateway/index.js",
    "start:user": "node src/services/user/index.js",
    "start:restaurant": "node src/services/restaurant/index.js",
    "start:order": "node src/services/order/index.js",
    "start:delivery": "node src/services/delivery/index.js",
    "start:tracking": "node src/services/tracking/index.js",
    "start:fraud": "node src/services/fraud/index.js",
    "start:payment": "node src/services/payment/index.js",
    "dev": "concurrently \"npm run start:user\" \"npm run start:restaurant\" \"npm run start:order\" \"npm run start:delivery\" \"npm run start:tracking\" \"npm run start:fraud\" \"npm run start:payment\" \"npm run start\"",
    "test": "jest"
  },
  "keywords": [
    "delivery",
    "restaurant",
    "microservices",
    "real-time",
    "tracking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "redis": "^4.6.7",
    "bull": "^4.11.5",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "geolib": "^3.3.4",
    "node-cache": "^5.1.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
